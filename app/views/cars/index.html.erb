<p id="notice"><%= notice %></p>

<h1>Cars</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cars.each do |car| %>
      <tr>
        <td><%= car.name %></td>
        <br>
        <td><%= image_tag(@car.image, style: "width:400px") %></td>
        <br>
        <td><%= link_to 'Show', car %></td>
        <td><%= link_to 'Edit Car', edit_car_path(car) %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
