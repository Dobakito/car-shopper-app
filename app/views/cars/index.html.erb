<p id="notice"><%= notice %></p>

<h1>Cars</h1>
<ul>
    <% @cars.each do |car| %>
        <h3><%= car.name %></h3>
        <br>
        <li><%= image_tag(car.image, style: "width:400px") %></li>
        <br>
        <h4>$<%= car.price %></h4>
        <br>
        <li><%= link_to 'Show', car %></li>
        <li><%= link_to 'Edit Car', edit_car_path(car) %></li>
        <br>
        <br>
    <% end %>
</ul>

<br>
